/* Logout Popover & Modal Styles */
.logout-popover.data-v-40ca6bb6 {
    position: absolute;
    top: 100rpx;
    right: 0;
    width: 320rpx;
    background: #fff;
    border-radius: 24rpx;
    box-shadow: 0 10rpx 40rpx rgba(0,0,0,0.1);
    padding: 32rpx;
    z-index: 100;
    border: 1px solid #f3f4f6;
    animation: fadeIn 0.2s ease-out;
}

.popover-nickname.data-v-40ca6bb6 {
    font-size: 32rpx;
    font-weight: 700;
    color: #1f2937;
    margin-bottom: 8rpx;
    display: block;
}

.popover-status.data-v-40ca6bb6 {
    display: flex;
    align-items: center;
    margin-bottom: 24rpx;
}

.status-dot.data-v-40ca6bb6 {
    width: 12rpx;
    height: 12rpx;
    background: #22c55e;
    border-radius: 50%;
    margin-right: 8rpx;
}

.status-text.data-v-40ca6bb6 {
    font-size: 24rpx;
    color: #059669;
}

.popover-btn.data-v-40ca6bb6 {
    width: 100%;
    background: #fef2f2;
    color: #ef4444;
    padding: 16rpx;
    border-radius: 12rpx;
    text-align: center;
    font-size: 26rpx;
    font-weight: 600;
}

.logout-modal-mask.data-v-40ca6bb6 {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.5);
    z-index: 999;
    display: flex;
    align-items: center;
    justify-content: center;
}

.logout-modal.data-v-40ca6bb6 {
    width: 560rpx;
    background: #fff;
    border-radius: 32rpx;
    padding: 48rpx 32rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
    animation: scaleIn 0.2s ease-out;
}

.logout-icon-circle.data-v-40ca6bb6 {
    width: 96rpx;
    height: 96rpx;
    background: #fef2f2;
    border-radius: 50%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-bottom: 32rpx;
}

.icon-lock.data-v-40ca6bb6 {
    font-size: 48rpx;
    color: #ef4444;
}

.logout-title.data-v-40ca6bb6 {
    font-size: 34rpx;
    font-weight: 700;
    color: #1f2937;
    margin-bottom: 16rpx;
}

.logout-desc.data-v-40ca6bb6 {
