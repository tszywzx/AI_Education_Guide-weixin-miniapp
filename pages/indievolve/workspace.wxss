    width: 100%;
    gap: 24rpx;
}

.btn-cancel.data-v-40ca6bb6 {
    flex: 1;
    background: #f3f4f6;
    color: #374151;
    font-size: 30rpx;
    font-weight: 600;
    border-radius: 16rpx;
    padding: 20rpx 0;
}

.btn-confirm-logout.data-v-40ca6bb6 {
    flex: 1;
    background: #fff;
    border: 2rpx solid #fecaca;
    color: #ef4444;
    font-size: 30rpx;
    font-weight: 600;
    border-radius: 16rpx;
    padding: 20rpx 0;
